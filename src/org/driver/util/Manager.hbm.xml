<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="org.driver.bean.Manager" table="manager">
        <id name="m_id" type="string" column="m_id">
            <generator class="assigned"/>
        </id>
        <property name="m_account" type="string" column="m_account"/>
        <property name="m_name" type="string" column="m_name" />
        <property name="m_pwd" type="string" column="m_pwd"/>
    </class>

</hibernate-mapping>
